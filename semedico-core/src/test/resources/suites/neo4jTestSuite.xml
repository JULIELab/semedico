<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Neo4jSuite" parallel="methods" thread-count="4">
    <test name="Neo4jTesting" enabled="true">
        <groups>
            <run>
                <include name="neo4j"/>
            </run>
        </groups>
        <classes>
            <class name="de.julielab.semedico.core.Neo4jTestContainer"/>
            <class name="de.julielab.semedico.core.search.query.translation.SecopiaQueryTranslatorIntegrationTest">
                <methods>
                    <include name="testConceptPhrase"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>